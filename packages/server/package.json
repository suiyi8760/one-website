{
  "name": "one-website-server",
  "version": "1.0.0",
  "description": "one website server side",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --no-notify --respawn --transpile-only src/index",
    "start": "cross-env NODE_ENV=production node dist/index",
    "build": "tsc",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:reset": "npx prisma migrate reset",
    "prisma:dev": "rushx migrate:dev && npx prisma generate",
    "gql:ts-gen": "graphql-codegen --config codegen.ts"
  },
  "keywords": [],
  "author": "johnny",
  "license": "ISC",
  "dependencies": {
    "fastify": "~4.11.0",
    "@prisma/client": "~4.8.1",
    "mercurius": "~11.5.0",
    "altair-fastify-plugin": "~5.0.9",
    "fastify-plugin": "~4.5.0",
    "graphql": "~16.6.0",
    "mercurius-codegen": "~5.0.1"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "typescript": "~4.9.4",
    "ts-node-dev": "~2.0.0",
    "ts-node": "~10.9.1",
    "prisma": "~4.8.0",
    "prettier": "~2.8.2",
    "cross-env": "~7.0.3"
  }
}
